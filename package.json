{
  "name": "claude-flow-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host localhost --port 8080",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode development --minify false",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --host localhost --port 8080",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "claude-flow:start": "npm run dev",
    "claude-flow:build": "npm run type-check && npm run lint && npm run build",
    "claude-flow:preview": "npm run build && npm run preview"
